---
import RootLayout from "./RootLayout.astro";
export interface Props {
  title: string;
}
const { title } = Astro.props;
---

<RootLayout {title}>
  <slot />
</RootLayout>

<script>
  if ("serviceWorker" in navigator) {
      navigator.serviceWorker.register("/serviceWorker.js", { scope: "/home" })
    }
</script>

<style>
  body {
    font-family: Inter;
  }
</style>
