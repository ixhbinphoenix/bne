---
import RootLayout from './RootLayout.astro';

export interface Props {
  title: string
}
const {title} = Astro.props;
---

<RootLayout title={title}>
  <slot></slot>
</RootLayout>

<style lang="scss">
  html {
    margin: 0px;
  }
  body {
    margin: 0px;
    background-color: #24273a;
  }
</style>
